@inproceedings{Wang2015a,
	title        = {{Unsupervised Temporal Segmentation of Repetitive Human Actions Based on Kinematic Modeling and Frequency Analysis}},
	author       = {Wang, Qifei and Kurillo, Gregorij and Ofli, Ferda and Bajcsy, Ruzena},
	year         = 2015,
	month        = 11,
	booktitle    = {Proceedings - 2015 International Conference on 3D Vision, 3DV 2015},
	publisher    = {Institute of Electrical and Electronics Engineers Inc.},
	pages        = {562--570},
	doi          = {10.1109/3DV.2015.69},
	isbn         = 9781467383325,
	abstract     = {In this paper, we propose a method for temporal segmentation of human repetitive actions based on frequency analysis of kinematic parameters, zero-velocity crossing detection, and adaptive k-means clustering. Since the human motion data may be captured with different modalities which have different temporal sampling rate and accuracy (e.g., Optical motion capture systems vs. Microsoft Kinect), we first apply a generic full-body kinematic model with an unscented Kalman filter to convert the motion data into a unified representation that is robust to noise. Furthermore, we extract the most representative kinematic parameters via the primary frequency analysis. The sequences are segmented based on zero-velocity crossing of the selected parameters followed by an adaptive k-means clustering to identify the repetition segments. Experimental results demonstrate that for the motion data captured by both the motion capture system and the Microsoft Kinect, our proposed algorithm obtains robust segmentation of repetitive action sequences.},
	archiveprefix = {arXiv},
