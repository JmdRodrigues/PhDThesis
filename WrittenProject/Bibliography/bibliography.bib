@inproceedings{Ordonez2011,
	title        = {{Using modified multivariate bag-of-words models to classify physiological data}},
	author       = {Ordóñez, Patricia and Armstrong, Tom and Oates, Tim and Fackler, Jim},
	year         = 2011,
	booktitle    = {Proceedings - IEEE International Conference on Data Mining, ICDM},
	pages        = {534--539},
